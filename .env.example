# Root environment file for macrometric
# Copy this file to .env and update values as needed
#
# For Docker development: cp .env.example .env
# For local development: cp backend/.env.example backend/.env

# Database (used for local development without Docker)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/macrometric

# JWT Authentication
SECRET_KEY=your-secret-key-here-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7

# USDA API (optional - for food search with higher rate limits)
USDA_API_KEY=

# CORS
FRONTEND_URL=http://localhost:3000
